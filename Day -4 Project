pragma solidity ^0.4.17 < 0.6.12;

contract ReportCard{
    string public name;
    uint public rollno;
    string public batch;
    string public status;
    uint public markssub1;
    uint public markssub2;
    uint public markssub3;
    uint public markssub4;
    uint public markssub5;
    
    function ReportCard(string newName, uint newRollno, string newBatch, string newStatus, uint newMarkssub1, uint newMarkssub2, uint newMarkssub3, uint newMarkssub4, uint newMarkssub5) public {
        name = newName;
        rollno = newRollno;
        batch = newBatch;
        status = newStatus;
        markssub1 = newMarkssub1;
        markssub2 = newMarkssub2;
        markssub3 = newMarkssub3;
        markssub4 = newMarkssub4;
        markssub5 = newMarkssub5;
    }
    
     function setDetails(string newName, uint newRollno, string newBatch, string newStatus, uint newMarkssub1, uint newMarkssub2, uint newMarkssub3, uint newMarkssub4, uint newMarkssub5) public {
        name = newName;
        rollno = newRollno;
        batch = newBatch;
        status = newStatus;
        markssub1 = newMarkssub1;
        markssub2 = newMarkssub2;
        markssub3 = newMarkssub3;
        markssub4 = newMarkssub4;
        markssub5 = newMarkssub5;
    }
    function getDetails() public view returns(string, uint,string,string, uint, uint, uint, uint, uint){
        return (name, rollno, batch, status, markssub1, markssub2, markssub3, markssub4, markssub5);
    }
}


